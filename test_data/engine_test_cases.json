{
  "_filename": "engine_test_cases.json",
  "_version": "0.122.0-Beta",
  "_date": "2025-12-17",
  "test_cases": [
    {
      "name": "Scenario A: Disjoint Sets (No Overlap)",
      "inputs": {
        "StationA": ["M1", "M2"],
        "StationB": ["M3", "M4"]
      },
      "expected": {
        "universe_count": 4,
        "common_count": 0,
        "station_metrics": {
          "StationA": {"count": 2, "differential": 2, "missed": 2},
          "StationB": {"count": 2, "differential": 2, "missed": 2}
        }
      }
    },
    {
      "name": "Scenario B: Partial Overlap (Realistic)",
      "inputs": {
        "StationA": ["M1", "M2", "M3"],
        "StationB": ["M2", "M3", "M4"]
      },
      "expected": {
        "universe_count": 4,
        "common_count": 2,
        "station_metrics": {
          "StationA": {"count": 3, "differential": 1, "missed": 1},
          "StationB": {"count": 3, "differential": 1, "missed": 1}
        }
      }
    },
    {
      "name": "Scenario C: Unknown Filtering",
      "inputs": {
        "StationA": ["M1", "Unknown"],
        "StationB": ["M1", "M2"]
      },
      "expected": {
        "universe_count": 2,
        "common_count": 1,
        "station_metrics": {
          "StationA": {"count": 1, "differential": 0, "missed": 1},
          "StationB": {"count": 2, "differential": 1, "missed": 0}
        }
      }
    }
  ]
}