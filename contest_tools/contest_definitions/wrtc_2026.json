        "SentRST",
        "SentZone",
        "Call",
        "RST",
        "RcvdMult",
        "Transmitter"
      ]
    }
  },
  "mutually_exclusive_mults": [
    [
      "Mult_DXCC",
      "Mult_HQ",
      "Mult_Official"
    ]
  ],
  "multiplier_rules": [
    {
      "name": "Countries",
      "value_column": "Mult_DXCC",
      "name_column": "Mult_DXCCName",
      "totaling_method": "once_per_band_no_mode"
    },
    {
      "name": "HQ Stations",
      "value_column": "Mult_HQ",
      "totaling_method": "once_per_band_no_mode"
    },
    {
      "name": "IARU Officials",
      "value_column": "Mult_Official",
      "totaling_method": "once_per_band_no_mode"
    }
  ],
  "default_qso_columns": [
