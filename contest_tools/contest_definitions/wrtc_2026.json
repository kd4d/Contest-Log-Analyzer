  "contest_name": "WRTC-2026",
  "scoring_module": "wrtc_2026_scoring",
  "contest_period": {
    "start_day": "Saturday",
    "start_time": "12:00:00 UTC",
    "end_day": "Sunday",
    "end_time": "11:59:59 UTC"
