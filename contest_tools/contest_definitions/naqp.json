{
  "_filename": "contest_tools/contest_definitions/naqp.json",
  "_version": "0.40.7-Beta",
  "_date": "2025-08-27",
  "contest_name": "NAQP",
  "valid_bands": [
    "160M",
    "80M",
    "40M",
    "20M",
    "15M",
    "10M"
  ],
  "dupe_check_scope": "per_band",
  "score_formula": "qsos_times_mults",
  "enable_adif_export": true,
  "custom_multiplier_resolver": "naqp_multiplier_resolver",
  "mutually_exclusive_mults": [
    [
      "Mult_STPROV",
      "Mult_NADXCC"
    ]
  ],
  "contest_specific_event_id_resolver": "naqp_event_id_resolver",
  "exchange_parsing_rules": {
    "NAQP-CW": {
      "regex": "([A-Z0-9]+)\\s+([A-Z]{2,4})\\s+([A-Z0-9/]+)\\s+([A-Z0-9]+)\\s+([A-Z]{2,4})(?:\\s+(\\d))?",
      "groups": [
        "SentName",
        "SentLocation",
        "Call",
        "RcvdName",
        "RcvdLocation",
        "Transmitter"
      ]
    },
    "NAQP-PH": {
      "regex": "([A-Z0-9]+)\\s+([A-Z]{2,4})\\s+([A-Z0-9/]+)\\s+([A-Z0-9]+)\\s+([A-Z]{2,4})(?:\\s+(\\d))?",
      "groups": [
        "SentName",
        "SentLocation",
        "Call",
        "RcvdName",
        "RcvdLocation",
        "Transmitter"
      ]
    },
    "NAQP-RTTY": {
      "regex": "([A-Z0-9]+)\\s+([A-Z]{2,4})\\s+([A-Z0-9/]+)\\s+([A-Z0-9]+)\\s+([A-Z]{2,4})(?:\\s+(\\d))?",
      "groups": [
        "SentName",
        "SentLocation",
        "Call",
        "RcvdName",
        "RcvdLocation",
        "Transmitter"
      ]
    }
  },
  "multiplier_rules": [
    {
      "name": "STPROV",
      "value_column": "Mult_STPROV",
      "name_column": "Mult_STPROVName",
      "totaling_method": "once_per_band"
    },
    {
      "name": "NA DXCC",
      "value_column": "Mult_NADXCC",
      "name_column": "Mult_NADXCCName",
      "totaling_method": "once_per_band"
    }
  ],
  "default_qso_columns": [
    "ContestName",
    "MyCall",
    "Frequency",
    "Mode",
    "Datetime",
    "SentRST",
    "SentName",
    "SentLocation",
    "Call",
    "RcvdRST",
    "RcvdName",
    "RcvdLocation",
    "Transmitter",
    "Band",
    "Date",
    "Hour",
    "Dupe",
    "DXCCName",
    "DXCCPfx",
    "CQZone",
    "ITUZone",
    "Continent",
    "Lat",
    "Lon",
    "Tzone",
    "Run",
    "QSOPoints",
    "Mult_STPROV",
    "Mult_STPROVName",
    "Mult_NADXCC",
    "Mult_NADXCCName"
  ]
}