  "exchange_parsing_rules": {
    "NAQP-CW": [
      {
        "regex": "([A-Z]+)\\s+([A-Z0-9/]+)\\s+([A-Z0-9/]+)\\s+([A-Z]+)\\s+([A-Z0-9/]+)(?:\\s+(\\d))?",
        "groups": [
          "SentName",
          "SentLocation",
          "Call",
          "RcvdName",
          "RcvdLocation",
          "Transmitter"
        ]
      },
      {
        "regex": "([A-Z]+)\\s+([A-Z0-9/]+)(?:\\s+(\\d))?",
        "groups": [
          "RcvdName",
          "RcvdLocation",
          "Transmitter"
        ]
      }
    ],
    "NAQP-SSB": [
      {
        "regex": "([A-Z]+)\\s+([A-Z0-9/]+)\\s+([A-Z0-9/]+)\\s+([A-Z]+)\\s+([A-Z0-9/]+)(?:\\s+(\\d))?",
        "groups": [
          "SentName",
          "SentLocation",
          "Call",
          "RcvdName",
          "RcvdLocation",
          "Transmitter"
        ]
      },
      {
        "regex": "([A-Z]+)\\s+([A-Z0-9/]+)(?:\\s+(\\d))?",
        "groups": [
          "RcvdName",
          "RcvdLocation",
          "Transmitter"
        ]
      }
    ]
  },
  "multiplier_rules": [
