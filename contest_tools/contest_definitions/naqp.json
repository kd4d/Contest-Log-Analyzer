{
  "_filename": "contest_tools/contest_definitions/naqp.json",
  "_version": "0.29.0-Beta",
  "_date": "2025-08-03",
  "contest_name": "NAQP",
  "dupe_check_scope": "per_band",
  "custom_multiplier_resolver": "naqp_multiplier_resolver",
  "exchange_parsing_rules": {
    "NAQP-CW": {
      "regex": "(\\d{3})\\s+([A-Z]+)\\s+([A-Z0-9/]+)(?:\\s+(\\d))?",
      "groups": ["RST", "RcvdName", "RcvdLocation", "Transmitter"]
    },
    "NAQP-SSB": {
      "regex": "(\\d{2})\\s+([A-Z]+)\\s+([A-Z0-9/]+)(?:\\s+(\\d))?",
      "groups": ["RS", "RcvdName", "RcvdLocation", "Transmitter"]
    }
  },
  "multiplier_rules": [
    {
      "name": "STPROV",
      "source_column": "FinalMultiplier",
      "value_column": "Mult1",
      "totaling_method": "sum_by_band",
      "alias_file": "NAQPmults.dat"
    },
    {
      "name": "DXCC",
      "source": "dxcc",
      "value_column": "Mult2",
      "name_column": "Mult2Name",
      "totaling_method": "sum_by_band"
    }
  ],
  "default_qso_columns": [
    "ContestName", "CategoryOverlay", "CategoryOperator", "CategoryTransmitter", "MyCall",
    "Frequency", "Mode", "Datetime",
    "RST", "RS", "SentName", "SentLocation",
    "Call",
    "RcvdName", "RcvdLocation", "Transmitter",
    "Band", "Date", "Hour", "Dupe",
    "DXCCName", "DXCCPfx", "CQZone", "ITUZone", "Continent", "WAEName", "WAEPfx", "Lat", "Lon", "Tzone",
    "Run", "QSOPoints",
    "Mult1", "Mult1Name", "Mult2", "Mult2Name", "FinalMultiplier"
  ]
}