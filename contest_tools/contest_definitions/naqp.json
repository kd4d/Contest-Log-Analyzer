  "contest_name": "NAQP",
  "valid_bands": [
    "160M",
    "80M",
    "40M",
    "20M",
    "15M",
    "10M"
  ],
  "dupe_check_scope": "per_band",
  "score_formula": "qsos_times_mults",
  "custom_multiplier_resolver": "naqp_multiplier_resolver",
  "contest_specific_event_id_resolver": "naqp_event_id_resolver",
  "exchange_parsing_rules": {
    "NAQP-CW": {
      "regex": "([A-Z0-9/]+)\\s+([A-Z0-9]+)\\s+([A-Z]{2,4})",
      "groups": [
        "Call",
        "SentName",
        "SentLocation"
      ]
    },
    "NAQP-PH": {
      "regex": "([A-Z0-9/]+)\\s+([A-Z0-9]+)\\s+([A-Z]{2,4})",
      "groups": [
        "Call",
        "SentName",
        "SentLocation"
      ]
    },
    "NAQP-RTTY": {
      "regex": "([A-Z0-9/]+)\\s+([A-Z0-9]+)\\s+([A-Z]{2,4})",
      "groups": [
        "Call",
        "SentName",
        "SentLocation"
      ]
    }
  },
  "multiplier_rules": [
    {
      "name": "Multipliers",
      "value_column": "Mult1",
      "totaling_method": "once_per_band"
    }
  ],
  "default_qso_columns": [
