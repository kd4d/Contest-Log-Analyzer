{
  "_filename": "contest_tools/contest_definitions/cq_ww_rtty.json",
  "contest_name": "CQ-WW-RTTY",
  "scoring_module": "cq_ww_rtty_scoring",
  "custom_multiplier_resolver": "cq_ww_rtty_multiplier_resolver",
  "valid_bands": [
    "80M",
    "40M",
    "20M",
    "15M",
    "10M"
  ],
  "dupe_check_scope": "per_band",
  "score_formula": "points_times_mults",
  "mults_from_zero_point_qsos": true,
  "enable_adif_export": true,
  "custom_adif_exporter": "cq_ww_adif",
  "contest_period": {
    "start_day": "Saturday",
    "start_time": "00:00:00 UTC",
    "end_day": "Sunday",
    "end_time": "23:59:59 UTC"
  },
  "excluded_reports": [
    "text_wae_score_report",
    "text_wae_comparative_score_report"
  ],
  "exchange_parsing_rules": {
    "CQ-WW-RTTY": [
      {
        "regex": "(\\d{3})\\s+(\\d{1,2})\\s+([A-Z]{2,})\\s+([A-Z0-9/]+)\\s+(\\d{3})\\s+(\\d{1,2})\\s+([A-Z]{2,})(?:\\s+(\\d))?",
        "groups": ["SentRST", "SentZone", "SentLocation", "Call", "RST", "Zone", "RcvdLocation", "Transmitter"]
      }
    ]
  },
  "multiplier_rules": [
    {
      "name": "Countries",
      "source": "wae_dxcc",
      "value_column": "Mult1",
      "name_column": "Mult1Name"
    },
    {
      "name": "Zones",
      "source_column": "Zone",
      "value_column": "Mult2"
    },
    {
      "name": "W/VE QTHs",
      "value_column": "Mult3",
      "name_column": "Mult3Name"
    }
  ]
}
