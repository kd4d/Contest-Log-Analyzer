{
  "_filename": "contest_tools/contest_definitions/iaru_hf.json",
  "_version": "0.91.16-Beta",
  "_date": "2025-10-10",
  "contest_name": "IARU-HF",
  "scoring_module": "iaru_hf_scoring",
  "contest_period": {
    "start_day": "Saturday",
    "start_time": "12:00:00 UTC",
    "end_day": "Sunday",
    "end_time": "11:59:59 UTC"
  },
  "dupe_check_scope": "per_band",
  "score_formula": "points_times_mults",
  "enable_adif_export": true,
  "custom_parser_module": "iaru_hf_parser",
  "custom_multiplier_resolver": "iaru_hf_multiplier_resolver",
  "custom_adif_exporter": "iaru_hf_adif",
  "excluded_reports": [
    "text_wae_score_report",
    "text_wae_comparative_score_report"
  ],
  "multiplier_rules": [
    {
      "name": "Zones",
      "value_column": "Mult_Zone",
      "totaling_method": "sum_by_band"
    },
    {
      "name": "HQ Stations",
      "value_column": "Mult_HQ",
      "totaling_method": "sum_by_band"
    },
    {
      "name": "IARU Officials",
      "value_column": "Mult_Official",
      "totaling_method": "sum_by_band"
    }
  ],
  "mutually_exclusive_mults": [
    [
      "Mult_Zone",
      "Mult_HQ",
      "Mult_Official"
    ]
  ],
  "exchange_parsing_rules": {
    "IARU-HF-ZONE-LOGGER": {
      "regex": "(\\d{2,3})\\s+(\\d{1,2})\\s+([A-Z0-9/]+)\\s+(\\d{2,3})\\s+([A-Z0-9]+)(?:\\s+(\\d))?",
      "groups": [
        "SentRST",
        "SentZone",
        "Call",
        "RST",
        "RcvdMult",
        "Transmitter"
      ]
    },
    "IARU-HF-HQ-LOGGER": {
      "regex": "(\\d{2,3})\\s+([A-Z]+)\\s+([A-Z0-9/]+)\\s+(\\d{2,3})\\s+([A-Z0-9]+)(?:\\s+(\\d))?",
      "groups": [
        "SentRST",
        "SentIARU",
        "Call",
        "RST",
        "RcvdMult",
        "Transmitter"
      ]
    }
  },
  "default_qso_columns": [
    "ContestName",
    "MyCall",
    "Frequency",
    "Mode",
    "Datetime",
    "SentRST",
    "SentRS",
    "SentZone",
    "SentIARU",
    "Call",
    "RST",
    "RS",
    "RcvdMult",
    "Transmitter",
    "Band",
    "Date",
    "Hour",
    "Dupe",
    "DXCCName",
    "DXCCPfx",
    "CQZone",
    "ITUZone",
    "Continent",
    "Run",
    "QSOPoints",
    "Mult_Zone",
    "Mult_HQ",
    "Mult_Official"
  ]
}