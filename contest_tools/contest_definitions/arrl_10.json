{
  "_filename": "contest_tools/contest_definitions/arrl_10.json",
  "_version": "0.90.0-Beta",
  "_date": "2025-09-13",
  "contest_name": "ARRL-10",
  "valid_bands": [
    "10M"
  ],
  "dupe_check_scope": "per_band",
  "multiplier_report_scope": "per_mode",
  "enable_adif_export": true,
  "custom_parser_module": "arrl_10_parser",
  "custom_multiplier_resolver": "arrl_10_multiplier_resolver",
  "excluded_reports": [
    "text_wae_score_report",
    "text_wae_comparative_score_report"
  ],
  "mutually_exclusive_mults": [
    [
      "Mult_State",
      "Mult_VE",
      "Mult_XE",
      "Mult_DXCC",
      "Mult_ITU"
    ]
  ],
  "exchange_parsing_rules": {
    "ARRL-10-DX-LOGGER": {
      "regex": "(\\d{2,3})\\s+(\\d+)\\s+([A-Z0-9/]+)\\s+(\\d{2,3})\\s+(.*)",
      "groups": [
        "SentRST",
        "SentSerial",
        "Call",
        "RST",
        "RcvdExchangeFull"
      ]
    },
    "ARRL-10-WVE-LOGGER": {
      "regex": "(\\d{2,3})\\s+([A-Z]{2,4})\\s+([A-Z0-9/]+)\\s+(\\d{2,3})\\s+(.*)",
      "groups": [
        "SentRST",
        "SentLocation",
        "Call",
        "RST",
        "RcvdExchangeFull"
      ]
    },
    "ARRL-10-RCVD-MM": {
      "regex": "(\\d)",
      "groups": [
        "RcvdITU"
      ]
    },
    "ARRL-10-RCVD-WVE": {
      "regex": "([A-Z]{2,4})",
      "groups": [
        "RcvdLocation"
      ]
    },
    "ARRL-10-RCVD-DX": {
      "regex": "(\\d+)",
      "groups": [
        "RcvdSerial"
      ]
    }
  },
  "multiplier_rules": [
    {
      "name": "States",
      "value_column": "Mult_State",
      "name_column": "Mult_StateName",
      "totaling_method": "once_per_mode"
    },
    {
      "name": "Provinces",
      "value_column": "Mult_VE",
      "name_column": "Mult_VEName",
      "totaling_method": "once_per_mode"
    },
    {
      "name": "Mexican States",
      "value_column": "Mult_XE",
      "name_column": "Mult_XEName",
      "totaling_method": "once_per_mode"
    },
    {
      "name": "DXCC",
      "value_column": "Mult_DXCC",
      "name_column": "Mult_DXCCName",
      "totaling_method": "once_per_mode"
    },
    {
      "name": "ITU Regions",
      "value_column": "Mult_ITU",
      "totaling_method": "once_per_mode"
    }
  ],
  "default_qso_columns": [
    "ContestName",
    "MyCall",
    "Frequency",
    "Mode",
    "Datetime",
    "SentRST",
    "SentSerial",
    "SentLocation",
    "Call",
    "RST",
    "RcvdExchangeFull",
    "RcvdSerial",
    "RcvdLocation",
    "RcvdITU",
    "Band",
    "Date",
    "Hour",
    "Dupe",
    "DXCCName",
    "DXCCPfx",
    "CQZone",
    "ITUZone",
    "Continent",
    "Run",
    "QSOPoints",
    "Mult_State",
    "Mult_StateName",
    "Mult_VE",
    "Mult_VEName",
    "Mult_XE",
    "Mult_XEName",
    "Mult_DXCC",
    "Mult_DXCCName",
    "Mult_ITU"
  ]
}