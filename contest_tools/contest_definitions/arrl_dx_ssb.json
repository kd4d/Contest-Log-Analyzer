{
  "_filename": "contest_tools/contest_definitions/arrl_dx_ssb.json",
  "contest_name": "ARRL-DX-SSB",
  "scoring_module": "arrl_dx_scoring",
  "contest_period": {
    "start_day": "Saturday",
    "start_time": "00:00:00 UTC",
    "end_day": "Sunday",
    "end_time": "23:59:59 UTC"
  },
  "enable_adif_export": true,
  "custom_parser_module": "arrl_dx_parser",
  "mults_from_zero_point_qsos": false,
  "custom_location_resolver": "arrl_dx_location_resolver",
  "custom_multiplier_resolver": "arrl_dx_multiplier_resolver",
  "multiplier_report_scope": "per_band",
  "excluded_reports": [
    "text_wae_score_report",
    "text_wae_comparative_score_report"
  ],
  "exchange_parsing_rules": {
    "ARRL-DX-SSB-W/VE": {
      "regex": "(\\d{2})\\s+([A-Z]{2})\\s+([A-Z0-9/]+)\\s+(\\d{2})\\s+([A-Z0-9]+)(?:\\s+(\\d))?",
      "groups": [
        "SentRS",
        "SentLocation",
        "Call",
        "RS",
        "RcvdPower",
        "Transmitter"
      ]
    },
    "ARRL-DX-SSB-DX": {
      "regex": "(\\d{2})\\s+([A-Z0-9]+)\\s+([A-Z0-9/]+)\\s+(\\d{2})\\s+([A-Z0-9]+)(?:\\s+(\\d))?",
      "groups": [
        "SentRS",
        "SentPower",
        "Call",
        "RS",
        "RcvdLocation",
        "Transmitter"
      ]
    }
  },
  "multiplier_rules": [
    {
      "name": "DXCC",
      "applies_to": "W/VE",
      "source_column": "Mult_DXCC",
      "source_name_column": "Mult_DXCCName",
      "value_column": "Mult1",
      "name_column": "Mult1Name",
      "totaling_method": "sum_by_band"
    },
    {
      "name": "STPROV",
      "applies_to": "DX",
      "source_column": "Mult_STPROV",
      "source_name_column": "STPROV_MultName",
      "value_column": "Mult1",
      "name_column": "Mult1Name",
      "totaling_method": "sum_by_band"
    }
  ],
  "default_qso_columns": [
    "ContestName",
    "CategoryOverlay",
    "CategoryOperator",
    "CategoryTransmitter",
    "MyCall",
    "Frequency",
    "Mode",
    "Datetime",
    "SentRS",
