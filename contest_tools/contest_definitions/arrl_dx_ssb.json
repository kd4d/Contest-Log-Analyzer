    "RS",
    "RcvdLocation",
    "RcvdPower",
    "Transmitter",
    "Band",
    "Date",
    "Hour",
    "Dupe",
    "DXCCName",
    "DXCCPfx",
    "CQZone",
    "ITUZone",
    "Continent",
    "WAEName",
    "WAEPfx",
    "Lat",
    "Lon",
    "Tzone",
    "Run",
    "QSOPoints",
    "Mult1",
    "Mult1Name",
    "Mult_STPROV",
    "STPROV_MultName",
    "Mult_DXCC",
    "Mult_DXCCName",
    "STPROV_IsNewMult",
    "DXCC_IsNewMult"
  ]
}