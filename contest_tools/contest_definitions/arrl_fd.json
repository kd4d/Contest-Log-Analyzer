{
  "_filename": "contest_tools/contest_definitions/arrl_fd.json",
  "_version": "0.90.0-Beta",
  "_date": "2025-09-13",
  "contest_name": "ARRL-FIELD-DAY",
  "valid_bands": [
    "160M", "80M", "40M", "20M", "15M", "10M", 
    "6M", "2M", "222MHz", "432MHz", "SAT"
  ],
  "dupe_check_scope": "per_band",
  "score_formula": "total_points",
  "excluded_reports": [
    "text_wae_score_report",
    "text_wae_comparative_score_report"
  ],
  "multiplier_rules": [
    {
      "name": "Sections",
      "value_column": "Mult_Section",
      "totaling_method": "once_per_mode"
    }
  ],
  "enable_adif_export": true,
  "custom_parser_module": "arrl_fd_parser",
  "custom_multiplier_resolver": "arrl_fd_multiplier_resolver",
  "exchange_parsing_rules": {
    "ARRL-FD": {
      "regex": "([A-Z0-9]+)\\s+([A-Z]{2,4})\\s+([A-Z0-9/]+)\\s+([A-Z0-9]+)\\s+([A-Z]{2,4})",
      "groups": ["SentClass", "SentSection", "Call", "RcvdClass", "RcvdSection"]
    }
  },
  "default_qso_columns": [
    "ContestName", "MyCall", "Frequency", "Mode", "Datetime",
    "SentClass", "SentSection",
    "Call", 
    "RcvdClass", "RcvdSection",
    "Band", "Date", "Hour", "Dupe", "DXCCName", "DXCCPfx", 
    "CQZone", "ITUZone", "Continent", "Run", "QSOPoints",
    "Mult_Section"
  ]
}