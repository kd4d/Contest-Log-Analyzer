{
  "_filename": "contest_tools/contest_definitions/cq_160.json",
  "_version": "0.49.1-Beta",
  "_date": "2025-08-24",
  "contest_name": "CQ-160",
  "enable_adif_export": true,
  "custom_parser_module": "cq_160_parser",
  "dupe_check_scope": "per_band",
  "valid_bands": [
    "160M"
  ],
  "custom_multiplier_resolver": "cq_160_multiplier_resolver",
  "operating_time_rules": {
    "min_off_time_minutes": 30,
    "single_op_max_hours": 30,
    "multi_op_max_hours": 40
  },
  "mutually_exclusive_mults": [
    ["Mult1", "Mult2"]
  ],
  "exchange_parsing_rules": {
    "CQ-160-CW": [
      {
        "regex": "(\\d{3})\\s+([A-Z]{2,3})\\s+([A-Z0-9/]+)\\s+(\\d{3})\\s+([A-Z]{2,3})(?:\\s+(\\d))?",
        "groups": ["SentRST", "SentLocation", "Call", "RST", "RcvdLocation", "Transmitter"]
      },
      {
        "regex": "(\\d{3})\\s+([A-Z]{2,3})\\s+([A-Z0-9/]+)\\s+(\\d{3})\\s+(\\d+)(?:\\s+(\\d))?",
        "groups": ["SentRST", "SentLocation", "Call", "RST", "RcvdZone", "Transmitter"]
      }
    ],
    "CQ-160-SSB": [
      {
        "regex": "(\\d{2})\\s+([A-Z]{2,3})\\s+([A-Z0-9/]+)\\s+(\\d{2})\\s+([A-Z]{2,3})(?:\\s+(\\d))?",
        "groups": ["SentRS", "SentLocation", "Call", "RS", "RcvdLocation", "Transmitter"]
      },
      {
        "regex": "(\\d{2})\\s+([A-Z]{2,3})\\s+([A-Z0-9/]+)\\s+(\\d{2})\\s+(\\d+)(?:\\s+(\\d))?",
        "groups": ["SentRS", "SentLocation", "Call", "RS", "RcvdZone", "Transmitter"]
      }
    ]
  },
  "multiplier_rules": [
    {
      "name": "STPROV",
      "source_column": "STPROV_Mult",
      "value_column": "Mult1",
      "totaling_method": "once_per_contest"
    },
    {
      "name": "DXCC",
      "applies_to": "W/VE",
      "source_column": "DXCC_Mult",
      "value_column": "Mult2",
      "name_column": "DXCC_MultName",
      "totaling_method": "once_per_contest"
    }
  ],
  "default_qso_columns": [
    "ContestName", "CategoryOverlay", "CategoryOperator", "CategoryTransmitter", "MyCall",
    "Frequency", "Mode", "Datetime",
    "RST", "RS", "SentLocation", "SentZone",
    "Call",
    "RcvdLocation", "RcvdZone", "Transmitter",
    "Band", "Date", "Hour", "Dupe",
    "DXCCName", "DXCCPfx", "CQZone", "ITUZone", "Continent", "WAEName", "WAEPfx", "Lat", "Lon", "Tzone",
    "Run", "QSOPoints",
    "Mult1", "Mult1Name", "Mult2", "Mult2Name", "STPROV_Mult", "DXCC_Mult", "DXCC_MultName"
  ]
}